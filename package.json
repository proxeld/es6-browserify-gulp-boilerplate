{
  "name": "y",
  "version": "0.0.1",
  "description": "Template for the project using ES6, Gulp and Browserify",
  "scripts": {
    "build": "gulp js",
    "watch": "gulp watch",
    "test": "mocha --compilers es6:babel-register --require babel-polyfill --recursive test",
    "browser-test": "browserify test/*.es6 -o build/tests-bundle.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "es6",
    "babel",
    "gulp",
    "browserify",
    "watchify"
  ],
  "author": "Maciej (proxeld) Urbanek",
  "license": "MIT License",
  "devDependencies": {
    "gulp": "~3.9.1",
    "gulp-util": "~3.0.7",
    "gulp-uglify": "~2.0.0",
    "browserify": "~13.1.0",
    "vinyl-source-stream": "~1.1.0",
    "babelify": "~7.3.0",
    "watchify": "~3.7.0",
    "gulp-rename": "~1.2.2",
    "vinyl-buffer": "~1.0.0",
    "phantomjs": "~2.1.7",
    "mocha": "~3.0.2",
    "babel-polyfill": "~6.13.0",
    "babel-register": "~6.14.0",
    "chai": "~3.5.0"
  },
}
